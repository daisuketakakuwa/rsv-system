{
  "name": "rsv-system",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:react": "vite --port 3000",
    "dev:server": "ts-node --project ./tsconfig.server.json ./server/server.ts",
    "dev": "npm-run-all --parallel --print-label dev:react dev:server",
    "build:react": "vite build",
    "build:server": "tsc --project ./tsconfig.server.json",
    "build": "npm-run-all build:react build:server",
    "preview": "vite preview",
    "lint": "eslint src/**/*"
  },
  "dependencies": {
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
