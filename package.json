{
  "name": "rsv-system",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:react": "vite --port 3000",
    "dev:server": "ts-node --project ./tsconfig.server.json --files ./server/server.ts",
    "dev": "npm-run-all --parallel --print-label dev:react dev:server",
    "build:react": "vite build",
    "build:server": "tsc --project ./tsconfig.server.json",
    "build": "npm-run-all --parallel --print-label build:react build:server",
    "start": "node ./build-server/server.js",
    "preview": "vite preview",
    "lint": "eslint src/**/*"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "firebase": "^10.7.1",
    "moment-timezone": "^0.5.43",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "styled-components": "^6.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/express-winston": "^4.0.0",
    "@types/firebase": "^3.2.1",
    "@types/moment-timezone": "^0.5.30",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
